include config.jade
- var idStr = ''
each video in videos
  - idStr += video + ','
- idStr = idStr.slice(videos[0].length+1, idStr.length-1)
html
  head
    link(rel='stylesheet', href='css/style.css')
    title whatisleantv
  body
    p.meta#title LeanTV
    p.meta#link whatisleantv.github.io
    main
      block content
